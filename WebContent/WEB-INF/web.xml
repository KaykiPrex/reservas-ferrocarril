<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>SistemaFrancesssdada</display-name>
	<welcome-file-list>
		<welcome-file>ComprarPasajes.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>ControladorListaViajes</servlet-name>
		<servlet-class>controladores.ControladorListaViajes</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ControladorListaViajes</servlet-name>
		<url-pattern>/ControladorListaViajes</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ControladorListaPasaje</servlet-name>
		<servlet-class>controladores.ControladorListaPasaje</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ControladorListaPasaje</servlet-name>
		<url-pattern>/ControladorListaPasaje</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>ControladorComprarPasaje</servlet-name>
		<servlet-class>controladores.ControladorComprarPasaje</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ControladorComprarPasaje</servlet-name>
		<url-pattern>/ControladorComprarPasaje</url-pattern>
	</servlet-mapping>
</web-app>